= simple_form_for(@admin_healgroup) do |f|
  = render 'shared/error_messages', object: @admin_healgroup

  .form-inputs
    = f.label 'Group Name:'
    = f.input :name
    = f.label 'Start Date:'
    = f.input :start_date, :order => [:month, :day, :year]

  .form-actions
    = f.button :button, t('.create'), type: :submit
